<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.AcademyMapper">

	<select id="Academy" parameterType="AcademyDTO" resultType="AcademyDTO">
		select a.* from academy a, u_member m where a.exercise = m.exercise and a.address1=m.address1 and m.id = #{id}
	</select>


</mapper> 
	